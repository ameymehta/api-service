{
  "info": {
    "description": "This is a REST API service",
    "termsOfService": "",
    "title": "API Service",
    "version": "1.0.0"
  },
  "openapi": "3.0.0",
  "servers": [
    {
      "url": "http://localhost:8088/api"
    }
  ],
  "tags": [
    {
      "description": "",
      "name": "api"
    }
  ],
  "components": {
    "schemas": {
      "ErrorContainer": {
        "properties": {
          "errors": {
            "items": {
              "$ref": "#/components/schemas/ErrorModel"
            },
            "type": "array"
          },
          "statusCode": {
            "type": "integer"
          }
        },
        "required": ["errors"],
        "type": "object"
      },
      "ErrorModel": {
        "properties": {
          "additionalProperties": {
            "type": "object"
          },
          "code": {
            "description": "The code associated with the error.",
            "type": "string"
          },
          "message": {
            "description": "A string describing the error.",
            "type": "string"
          },
          "moreInfo": {
            "description": "Detailed information about the error.",
            "type": "string"
          }
        },
        "required": ["code", "message"],
        "type": "object"
      },
      "Object": {
        "properties": {
          "description": {
            "description": "A description of the object.",
            "type": "string"
          },
          "id": {
            "format": "uuid",
            "type": "string",
            "readOnly": true
          },
          "name": {
            "description": "The name of the object",
            "type": "string"
          }
        },
        "required": ["name"],
        "type": "object"
      },
      "Objects": {
        "properties": {
          "objects": {
            "items": {
              "$ref": "#/components/schemas/Object"
            },
            "type": "array"
          },
          "totalCount": {
            "type": "integer"
          }
        },
        "type": "object"
      }
    }
  },
  "paths": {
    "/objects": {
      "get": {
        "description": "Returns a list of objects stored in a file store.",
        "operationId": "listObjects",
        "responses": {
          "200": {
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Objects"
                }
              }
            },
            "description": "OK"
          },
          "400": {
            "description": "Bad Request"
          }
        },
        "summary": "Lists objects",
        "tags": ["api"]
      }
    }
  }
}
